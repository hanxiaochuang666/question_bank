<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包,8.0以上的包只会生成insert，所以需要低版本-->
    <classPathEntry
            location="D:\mavenSpace\maven_repo\mysql\mysql-connector-java\5.1.35\mysql-connector-java-5.1.35.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--自定义注释插件-->
        <plugin type="org.mybatis.customPlugin.model.NotesPlugin"/>
        <!--在去除注释的情况下 防止重复生成mapper语句插件-->
        <plugin type="org.mybatis.customPlugin.xml.IsRepeatPlugin"/>
        <!--添加自定义mapperSql插件-->
        <plugin type="org.mybatis.customPlugin.xml.MapperPlugin">
            <!--&lt;!&ndash;排序规则依赖&ndash;&gt;-->
            <!--<property name="orderSqlMap" value="com.jdd.common.orderSqlMap"/>-->
            <!--&lt;!&ndash;分页规则依赖&ndash;&gt;-->
            <!--<property name="pageSqlMap" value="com.jdd.common.pageSqlMap"/>-->
            <!--非空判断指定-->
            <property name="emptyClass" value="@org.springframework.util.StringUtils@isEmpty"/>
        </plugin>
        <!--model添加toString-->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <!--自定义生成service-->
        <!--<plugin type="org.mybatis.customPlugin.service.ServicePlugin" >-->
        <!--&lt;!&ndash;包路径&ndash;&gt;-->
        <!--<property name="targetPackage" value="cn.eblcu.sso.domain"/>-->
        <!--&lt;!&ndash;项目路径&ndash;&gt;-->
        <!--<property name="targetProject" value="src/main/java"/>-->
        <!--&lt;!&ndash;继承&ndash;&gt;-->
        <!--&lt;!&ndash;<property name="rootInterface" value="com.jdd.service.IBaseService"/>&ndash;&gt;-->
        <!--&lt;!&ndash;基础dao路径&ndash;&gt;-->
        <!--<property name="targetDaoPackage" value="cn.eblcu.sso.domain"/>-->
        <!--</plugin>-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/question_bank?useUnicode=true&amp;characterEncoding=UTF-8&amp;serverTimezone=UTC"
                        userId="root" password="root">
        </jdbcConnection>
        <!--不应强制使用BigDecimal字段-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="cn.eblcu.questionbank.persistence.entity.dto" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <!--<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">-->
            <!--<property name="enableSubPackages" value="true"/>-->
        <!--</sqlMapGenerator>-->
        <!-- 生成DAO的包名和位置-->
        <!--<javaClientGenerator type="XMLMAPPER" targetPackage="cn.eblcu.questionbank.persistence.dao"-->
                             <!--targetProject="src/main/java">-->
            <!--<property name="enableSubPackages" value="true"/>-->
        <!--</javaClientGenerator>-->

        <table tableName="test_result_detail"
               enableSelectByExample="false"
               enableDeleteByExample="false"
               enableCountByExample="false"
               enableUpdateByExample="false"
               selectByExampleQueryId="false"
               schema="root">
            <property name="ignoreQualifiersAtRuntime" value="false"/>
            <property name="useActualColumnNames" value="false"/>
        </table>
    </context>

</generatorConfiguration>